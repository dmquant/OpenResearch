# Gemini API Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_MODEL=gemini-2.5-flash

# Gemini Embedding Model
VITE_GEMINI_EMBEDDING_MODEL=gemini-embedding-001

# Cloudflare Worker API (optional - enables cloud features)
VITE_WORKER_API_URL=https://your-worker.your-subdomain.workers.dev

# Worker API Authentication (required for cloud mode)
VITE_WORKER_API_TOKEN=your_worker_api_token_here

# Development mode (set to 'cloud' to use Worker API, 'local' for direct API calls)
VITE_MODE=local